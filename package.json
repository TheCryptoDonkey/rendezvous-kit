{
  "name": "rendezvous-kit",
  "version": "1.11.4",
  "type": "module",
  "description": "Find fair meeting points for N people â€” isochrone intersection, venue search, and fairness scoring.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./geo": {
      "types": "./dist/geo.d.ts",
      "default": "./dist/geo.js"
    },
    "./engines/*": {
      "types": "./dist/engines/*.d.ts",
      "default": "./dist/engines/*.js"
    },
    "./venues": {
      "types": "./dist/venues.d.ts",
      "default": "./dist/venues.js"
    },
    "./rendezvous": {
      "types": "./dist/rendezvous.d.ts",
      "default": "./dist/rendezvous.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "bench": "vitest bench"
  },
  "dependencies": {
    "geohash-kit": "^1.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/npm": "^13.1.4",
    "semantic-release": "^25.0.3",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "llms.txt",
    "llms-full.txt"
  ],
  "keywords": [
    "rendezvous",
    "meeting-point",
    "isochrone",
    "fairness",
    "routing",
